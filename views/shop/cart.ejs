    <body>
        <%- include('../includes/navigation.ejs') %>
        <main>
            <% if (products.length>0) { %>
                <ul>
                    <% products.forEach(element => { %>
                     <li><%= element.productData.title %>(<%= element.qty %>) </li> 
                    <% }) %>
                </ul>
             <% } else { %>
              <h1>No Product in the cart</h1>
            <% } %>
            <h1></h1>
        </main>
